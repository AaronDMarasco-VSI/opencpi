#!/bin/bash
../OCLGen/bin/Debug/OCLGen "./components/matrixMul.ocl/matrixMul.xml" -max_num_dyn_local_buffs 2 -outdir  "./components/matrixMul.ocl"
../OCLcc/bin/Debug/OCLcc OCL_Worker.h ./components/matrixMul.ocl/gen/matrixMul_Worker.h ./components/matrixMul.ocl/gen/matrixMul_library.cl matrixMul.cl -o  ./components/matrixMul.ocl/matrixMul.bin -build_options -cl-fast-relaxed-math
